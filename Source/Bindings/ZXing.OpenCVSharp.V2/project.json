{
	"name": "ZXing.OpenCV.V2",
	"version": "0.16.3",
	"dependencies": {
		"OpenCvSharp-AnyCPU": "2.4.10.20170306"
	},
	"frameworks": {
		"net20": {
			"dependencies": {
				"zxing.net2.0": {
					"target": "project"
				}
			},
			"buildOptions": {
				"define": [ "NET20" ]
			}
		},
		"net35": {
			"dependencies": {
				"zxing.net3.5": {
					"target": "project"
				}
			},
			"buildOptions": {
				"define": [ "NET35" ]
			}
		},
		"net40": {
			"dependencies": {
				"zxing.net4.0": {
					"target": "project"
				}
			},
			"buildOptions": {
				"define": [ "NET40" ]
			}
		},
		"net45": {
			"dependencies": {
				"zxing.net4.5": {
					"target": "project"
				}
			},
			"buildOptions": {
				"define": [ "NET45" ]
			}
		}
	},
	"buildOptions": {
		"optimize": true,
		"xmlDoc": true,
		"platform": "anycpu",
		"publicSign": true,
		"delaySign": false,
		"keyFile": "../../../Key/private.snk",
		"debugType": "full",
		"outputName": "zxing.opencvsharp.v2",
		"allowUnsafe": true,
		"compile": {
			"includeFiles": [
				"Properties/AssemblyInfo.cs",
				"BarcodeReader.cs",
				"BarcodeReader.Extensions.cs",
				"MatLuminanceSource.cs"
			]
		}
	},
	"configurations": {
		"Debug": {
			"buildOptions": {
				"xmlDoc": false,
				"optimize": false,
		      "publicSign": false
			}
		}
	}
}
